/* pages/regist/regist.wxss */
.wrap{padding-top: 10px;font-size: 32rpx;}
.form{padding: 10px;line-height: 36px;}
.form-item{display: flex;border-bottom: 1px solid #ccc;height: 36px;}
.labelText{width: 200rpx;}
.form input{flex-grow: 1;height: 36px;line-height: 36px;}
.submit{margin-top: 60rpx;}
.securityCode{color: #1AAD19;width: 150rpx;text-align: right;font-size: 30rpx;padding: 0;background-color: transparent;}
.securityCode:after{border: none;background-color: transparent;}
.agreement{position: fixed;bottom: 15px;width: 100%;display: flex;justify-content: center;font-size: 24rpx;}
navigator{color: #00bcd4;}